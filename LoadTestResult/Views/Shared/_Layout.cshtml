    <script src="~/Content/js/Angular/Controller/highcharts.js"></script>
    <script src="~/Content/js/Angular/jquery.blockUI.min.js"></script>
    <script src="~/Content/js/Angular/angular.js"></script>

    <script src="~/Content/js/Angular/angular-route.js"></script>
    <script src="~/Content/js/Angular/Controller/checklist-model.js"></script>
    <script src="~/Content/js/Angular/Controller/jquery.growl.js"></script>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.11.0.js"></script>
    <script src="~/Content/js/Angular/App.js"></script>
    <script src="~/Content/js/Angular/Controller/LoadTestList.js"></script>
    <script src="~/Content/js/Angular/Controller/ChangePassword.js"></script>
    <script src="~/Content/js/Angular/Controller/DetailedResult.js"></script>
    <script src="~/Content/js/Angular/Controller/ErrorDetails.js"></script>
    <script src="~/Content/js/Angular/Controller/GraphRepesentation.js"></script>

