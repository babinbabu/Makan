<!DOCTYPE html>
<html lang="en" ng-app="LoadTestResult">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Load Test Result </title>

    <!-- Bootstrap -->
    <link href="~/Content/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/css/bootstrap/font-awesome.min.css" rel="stylesheet" />
    <!-- NProgress -->
    <link href="~/Content/css/bootstrap/nprogress.css" rel="stylesheet" />
    <!-- iCheck -->
    <link href="~/Content/css/bootstrap/green.css" rel="stylesheet" />
    <!-- bootstrap-progressbar -->
    <link href="~/Content/css/bootstrap/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" />
